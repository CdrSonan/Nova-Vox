#:kivy 2.0.0
<ParamCurve>:
    on_scroll_x: self.parent.triggerScroll()
    do_scroll: (True, False)
    scroll_type: ["bars"]
    bar_width: 10
    xScale: 1
    Widget:
        size_hint: (None, None)
        y: self.parent.y + 10
        width: 5000
        height: self.parent.height - 10
        canvas:
            Color:
                rgba: (0.5, 0.5, 0.5, 1)
            Line:
                points: [(0, self.parent.height / 2), (5000, self.parent.height / 2)]
            Line:
                points: [(0, 0), (5000, 0)]
            Line:
                points: [(0, self.parent.height), (5000, self.parent.height)]
